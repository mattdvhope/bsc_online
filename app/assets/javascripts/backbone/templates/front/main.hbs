<div class="front-main-hbs">

<div class="container-full main" id="page-top">
  <div class="row">
    <div class="col-md-12 backg">
      <div class="col-md-6 col-md-offset-3 inner col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-3">
        <div class="text-box">
          <div id="welcome-language">
            {{#if thai_flag_present}}
              {{> front/english/_top_welcome }}
            {{^}}
              {{> front/thai/_top_welcome }}
            {{/if}}
          </div>
          <div class="col-md-4">
            <span id="backbone-app">
              <p><a class="link-button" href="/log_in">{{login}}</a></p>

            </span>
          </div>
          <div class="col-md-4">
            <span id="backbone-app">
              <p><a class="link-button" href="/register_student">{{register}}</a></p>
            </span>
          </div>
          <div class="col-md-4">
            <p id="backbone-app"><a class="link-button" href="/volunteer_info">{{volunteer}}</a></p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-12 col-xs-12 some-notes">
      {{#if thai_flag_present}}
        {{> front/english/_middle_welcome }}
      {{^}}
        {{> front/thai/_middle_welcome }}
      {{/if}}
    </div>
  </div>
</div>


