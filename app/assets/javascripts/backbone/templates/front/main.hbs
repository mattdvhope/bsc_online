  <h2 class="text-center features-text">{{features}}</h2>
  <div class="row">
     <div class="col-xs-12 col-sm-4">  
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features1.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{!-- {{#if thai_language}}
              {{> front/thai/_city_talk_class }}
            {{^}}
              {{> front/english/_city_talk_class }}
            {{/if}} --}}
          </div>
        </div>
     </div>
     <div class="col-xs-12 col-sm-4"> 
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features2.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{#if thai_language}}
              {{> front/thai/_eng_conversation_partners }}
            {{^}}
              {{> front/english/_eng_conversation_partners }}
            {{/if}}
          </div>
        </div>
     </div>
     <div class="col-xs-12 col-sm-4">
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features1.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{#if thai_language}}
              {{> front/thai/_community_boards }}
            {{^}}
              {{> front/english/_community_boards }}
            {{/if}}
          </div>
        </div>
     </div>
  </div>
  <div class="row">
     <div class="col-xs-12 col-sm-4">  
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features4.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{#if thai_language}}
              {{> front/thai/_conversation_groups }}
            {{^}}
              {{> front/english/_conversation_groups }}
            {{/if}}
          </div>
        </div>
     </div>
     <div class="col-xs-12 col-sm-4">
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features4.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{#if thai_language}}
              {{> front/thai/_online_classes }}
            {{^}}
              {{> front/english/_online_classes }}
            {{/if}}
          </div>
        </div>
     </div>
     <div class="col-xs-12 col-sm-4"> 
        <div class="picturing">
          <img src="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Features6.png" alt="Class Scene" align="middle" class="img-responsive">
          <div class="col-md-12 col-sm-offset-1 col-md-offset-1 col-lg-offset-2 col-sm-10 icon-text-box">
            {{#if thai_language}}
              {{> front/thai/_ongoing_opportunities }}
            {{^}}
              {{> front/english/_ongoing_opportunities }}
            {{/if}}
          </div>
        </div>
     </div>
  </div>
  {{#if thai_language}}
    {{> front/thai/_in_addition }}
  {{^}}
    {{> front/english/_in_addition }}
  {{/if}}
</div> <!-- Features -->


<!-- Get Started -->
