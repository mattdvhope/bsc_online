            </div>
            {{!-- <h3>{{step_two}}</h3> --}}
          </a>
        </div>
        <div>
          {{#if thai_language}}
            {{> front/thai/_helping_friends_text }}
          {{^}}
            {{> front/english/_helping_friends_text }}
          {{/if}}
