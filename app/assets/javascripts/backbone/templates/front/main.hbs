        {{^}}
          {{> front/english/_get_started }}
        {{/if}}
        <div class="col-md-5 col-sm-12 col-xs-12 work-space">
          <a href="https://s3-ap-southeast-1.amazonaws.com/ccmcoversbsc/Portrait.png" data-lightbox="image-1">
            <div class="featured-img">
